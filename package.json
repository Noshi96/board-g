{
  "name": "board-g",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:app": "turbo run dev --filter=web",
    "lint": "turbo run lint",
    "prettier": "prettier -c \"**/*.{ts,tsx,md}\" --no-color",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint-config-custom": "*",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "2.8.7",
    "ts-node": "^10.9.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.0"
}
